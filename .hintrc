{
  "extends": [
    "development"
  ],
  "hints": {
    "typescript-config/consistent-casing": "off",
    "no-inline-styles": "off",
    "css-prefix-order": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "image-rendering: crisp-edges"
        ]
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "apple-touch-icons": "off"
  }
}